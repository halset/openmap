default: classes

all:
	cd ${OPENMAP_HOME}/src/openmap && ${OPENMAP_HOME}/bin/ant -emacs $@.openmap

classes:
	cd ${OPENMAP_HOME}/src/openmap && ${OPENMAP_HOME}/bin/ant -emacs $@.openmap

clean:
	cd ${OPENMAP_HOME}/src/openmap && ${OPENMAP_HOME}/bin/ant -emacs $@.openmap

distclean:
	cd ${OPENMAP_HOME}/src/openmap && ${OPENMAP_HOME}/bin/ant -emacs $@.openmap

docs:
	cd ${OPENMAP_HOME}/src/openmap && ${OPENMAP_HOME}/bin/ant -emacs $@.openmap

clean_docs:
	cd ${OPENMAP_HOME}/src/openmap && ${OPENMAP_HOME}/bin/ant -emacs $@.openmap

jar:
	cd ${OPENMAP_HOME}/src/openmap && ${OPENMAP_HOME}/bin/ant -emacs $@.openmap

help:
	cd ${OPENMAP_HOME}/src/openmap && ${OPENMAP_HOME}/bin/ant -emacs -projecthelp



