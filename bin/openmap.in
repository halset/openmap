#!/bin/sh
# @configure_input@
# 


DIR=`dirname $0` >/dev/null 2>&1
PRG=`basename $0` >/dev/null 2>&1
cd $DIR
PRG=`pwd`/$PRG >/dev/null 2>&1

TEST=/usr/bin/test

#
# If PRG is a symlink, trace it to the real home directory
#
while $TEST -L "$PRG"
do
    newprg=`expr "\`/bin/ls -l "$PRG"\`" : ".*$PRG -> \(.*\)"`
    expr "$newprg" : / >/dev/null || newprg="`dirname $PRG`/$newprg"
    PRG="$newprg"
done

OM_HOME=`dirname $PRG`/..
CLASSPATH=$OM_HOME:$OM_HOME/lib/openmap.jar:$OM_HOME/share:@SWING_JAR@
export CLASSPATH

@JAVA@ -mx64m -Dopenmap.configDir=$OM_HOME/share com.bbn.openmap.app.OpenMap
